				If gettarget(DllStructGetData($aagentarray[$i], "ID")) == DllStructGetData($aagent, "ID") Then
					If getdistance($aagentarray[$i], $aagent) < 5000 Then
						If DllStructGetData($aagentarray[$i], "Team") <> 0 Then
