			Local $lbuffstruct = DllStructCreate("long SkillId;byte unknown1[4];long BuffId;long TargetId")
			Local $lbuffcount = getbuffcount($aheronumber)
			Local $lbuffstructaddress
			Local $loffset[4]
			$loffset[0] = 0
