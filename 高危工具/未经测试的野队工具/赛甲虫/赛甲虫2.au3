							If $targetallegiance = 1 OR $targetallegiance = 4 OR $targetallegiance = 5 OR $targetallegiance = 6 Then ExitLoop
							$targetisdead = DllStructGetData(getcurrenttarget(), "Effects")
							If $targetisdead = 16 Then ExitLoop
