			For $i = 0 To $lcount[1] - 1
				$loffset[4] = 36 * $i
				$lbuffer = memoryreadptr($mbasepointer, $loffset)
				If $lbuffer[1] == getheroid($aheronumber) Then
					$loffset[4] = 4 + 36 * $i
					ReDim $loffset[6]
					For $j = 0 To $lbuffcount - 1
						$loffset[5] = 0 + 16 * $j
						$lbuffstructaddress = memoryreadptr($mbasepointer, $loffset)
