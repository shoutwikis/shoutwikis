							death()
						Next
					ElseIf $wert > 36000 Then
						For $i = 0 To 5
							Sleep($wert / 5)
							death()
						Next
					ElseIf $wert > 27000 Then
						For $i = 0 To 4
							Sleep($wert / 4)
							death()
						Next
					ElseIf $wert > 18000 Then
						For $i = 0 To 3
							Sleep($wert / 3)
							death()
						Next
					ElseIf $wert >= 9000 Then
						For $i = 0 To 2
							Sleep($wert / 2)
							death()
						Next
					Else
						Sleep($wert)
						death()
					EndIf
				EndFunc

